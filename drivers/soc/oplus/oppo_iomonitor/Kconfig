#ifdef VENDOR_EDIT
#/* Hank.liu@TECH.PLAT.Storage, 2020-02-18, add oppo iomonitor driver*/
config OPPO_IOMONITOR
	bool "Oppo iomonitor module"
	help
	define this config to init oppo iomonitor.

config IOMONITOR_WITH_F2FS
	bool "Filesystem is f2fs"
	depends on OPPO_IOMONITOR
	help
		define this config to support both f2fs and ext4
#endif /* VENDOR_EDIT */
